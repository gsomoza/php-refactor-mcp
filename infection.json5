{
    "$schema": "https://raw.githubusercontent.com/infection/infection/0.29.0/resources/schema.json",
    "source": {
        "directories": ["src"]
    },
    "logs": {
        "text": "infection.log",
        "html": "infection.html",
        "summary": "infection-summary.log",
        "github": true
    },
    "mutators": {
        "@default": true
    },
    "minMsi": 90,
    "minCoveredMsi": 90,
    "testFramework": "phpunit",
    "testFrameworkOptions": "--testsuite=default"
}
